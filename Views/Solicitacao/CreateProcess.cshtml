@model MvcSite.Models.ProcessoViewModel
@{
    ViewData["Title"] = "Processo";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h1>@ViewData["Title"]</h1>

@using (Html.BeginForm())
{
     @Html.AntiForgeryToken()
    <table class="table">
         <tr>
            <th>
                @Html.LabelFor(model => model.Key)                

            </th>
            <td>
                @Html.EditorFor(model => model.Key)
            </td>
        </tr>   
        <tr>
            <th>
                @Html.LabelFor(model => model.Nome)

            </th>
            <td>
                @Html.EditorFor(model => model.Nome)
            </td>
        </tr>      
           <tr>
            <th>
                @Html.LabelFor(model => model.id)

            </th>
            <td>
                @Html.DisplayFor(model => model.id)
            </td>
        </tr>       
        <tr>
            <td><input type="submit" value="Criar Processo" /></td>    
            <td>@Html.ActionLink("Back to List", "ListProcess", "Solicitacao")</td>
        </tr>
        <tr>
            <td>
                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
            </td>
        </tr>
    </table>
}
